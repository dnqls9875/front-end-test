{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "extends": ["plugin:prettier/recommended"],
  "rules": {
    "no-unused-vars": ["warn"],
    "no-undef": ["error"]
  },
  "globals": {
    "$": "readonly",
    "jQuery": "readonly",
    "AOS": "readonly",
    "Swiper": "readonly",
    "_": "readonly"
  }
}
